  "name": "rustaba",
  "version": "0.1.0",
  "scripts": {
    "start": "rimraf dist pkg && webpack-dev-server --open -d",
    "dev": "rimraf dist pkg && webpack-dev-server --env.dev --open -d",
    "build": "rimraf dist pkg && webpack --env.prod && docker build -t rustaba .",
    "deploy": "docker cp dist/. rustaba:/usr/local/apache2/htdocs/ && docker stop rustaba && docker start rustaba"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "vue": "^3.0.0-beta.2"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.2.45",
    "@wasm-tool/wasm-pack-plugin": "^1.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^2.7.2",
    "rimraf": "^3.0.0",
    "ts-loader": "^9.4.2",
    "typescript": "^3.9.5",
    "url-loader": "^4.0.0",
    "vue-loader": "^16.8.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.7.1"
  }
}
